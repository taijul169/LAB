                            <form action="/admin/createblog" enctype="multipart/form-data" method="POST" id="blogAddform" class="form-horizontal">
                                  <input type="hidden" name="_token" value="DtD3A24tSpfK65j9tt9dSFo5k8v6ylBlFCflJjM2">   <!--title-->
                                <div class="form-group">
                                    <label for="title" class="control-label col-sm-2">blog Title <span style="color:red">*</span></label>
                                    <div class="col-sm-12 ">
                                        <input type="text" id="title" name="title" class="form-control" required="required" value="" placeholder="blog Title">
                                    </div>
                                </div>

                                <!--description-->
                                <div class="form-group">
                                    <div class="col-sm-12"></div>
                                    <label for="description" class="control-label col-sm-2">blog Description <span style="color:red">*</span></label>
                                    <textarea name="details" required id="" cols="10" rows="10" class="form-control"></textarea>
                                </div>

                                <!--blog file-->
                                <div class="form-group" id="blogFile">
                                    <label for="blog_file" class="control-label col-sm-2">blog File</label>
                                    <div class="col-sm-12 ml-lg-2">
                                        <div class="row">
                                            <div class="col-sm-12 ">
                                               <input type="file" accept="image/png, image/jpeg" class="form-control" name="file">
                                                
                                            </div>
                                            
                                        </div>
                                        
                                    </div>

                                </div>
                                <div class="form-actions col-md-12">
                                    <button type="submit" class="btn btn-primary"><i class="fa fa-plus-circle mr-2"></i>Create
                                    </button>
                                    
                                </div>
                            </form>
                           </div>
                        </div>
