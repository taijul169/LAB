                                 </div>
                             </div>
                               <!-- Select Service -->
                               <div class="col-md-6 col-lg-6 col-xl-6 col-12 col-sm-12">
                                   <div class="form-group">
                                       <select id="service" required name="subject">
                                           <option value="">-Select Service</option>
                                           <option value="Membership">Membership</option>
                                           <option value="Registration">Registration</option>
                                           <option value="ID Card">ID Card</option>
                                           <option value="Payment">Payment</option>
                                       </select>
                                   </div>
                               </div>
                                 <!-- Subject -->
                             <div class="col-md-6 col-lg-6 col-xl-6 col-12 col-sm-12">
                                 <div class="form-group">
                                     <input type="text" placeholder="Subject" id="subject">
                                 </div>
                             </div>
                              <!-- Message -->
                             <div class="col-md-12 col-lg-12 col-xl-12 col-12 col-sm-12">
                                 <div class="form-group">
                                     <textarea name="details" id="u_message" cols="5" rows="3" placeholder="Message"></textarea>
                                 </div>
                             </div>
                               <!-- submit button -->
                               <div class="col-md-6 col-lg-6 col-xl-6 col-6 col-sm-6">
                                   <div class="form-group">
                                       <button type="submit" class="submit-message" id="submit">Send Message</button>
