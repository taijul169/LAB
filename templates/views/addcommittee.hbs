                               <label for="">transaction Id</label>
                               <input type="text" name="code" class="form-control " id="transactionid">
                           </div>
                           <div class="form-group">
                               <label for="">Sender No</label>
                               <input type="text" name="code" class="form-control " id="senderno">
                           </div>
                           <div class="form-group">
                               <label for="">Payment Date</label>
                               <input type="text" name="code" class="form-control " id="payment_date">
                           </div>
                      
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                    </div>
                    </div>
                  </form> 
