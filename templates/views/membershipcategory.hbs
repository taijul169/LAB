                      <script>

                                const name_el =  document.getElementById('name'); 
                                const id_el =  document.getElementById('id_category'); 
                                const price_el =  document.getElementById('price'); 
                                const remarks_el =  document.getElementById('remarks'); 
                                const membership_type_el =  document.getElementById('membership_type'); 
                                const categoryEdit = (id,name,price,type,remark)=>{
                                    id_el.value = id;
                                    name_el.value = name;
                                    price_el.value = price;
                                    membership_type_el.value = type;
                                    remarks_el.value = remark;  
                                }
                                //onsubmit
                                const categorysubmitbtn = document.getElementById("categorysubmitbtn");
                                categorysubmitbtn.addEventListener("submit",(e)=>{
                                    e.preventDefault();
                                    const id =  document.getElementById("id_category").value;
                                    const name =  document.getElementById("name").value;
                                    const price =  document.getElementById("price").value;
                                    const type =  document.getElementById("membership_type").value;
                                    const remark =  document.getElementById("remarks").value;
                                    window.location.href = `/admin/editcategory?id=${id}&name=${name}&price=${price}&type=${type}&remark=${remark}`;
                                    
                                })
                                
                            </script>
