        {{>nav}}
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb bg-white">
                <div class="row align-items-center">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                        <h4 class="page-title text-uppercase font-medium font-14">Create Message</h4>
                    </div>
                    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                        <div class="d-md-flex">
                            <ol class="breadcrumb ml-auto">
                                <li><a href="#">Dashboard</a></li>
                            </ol>
                            <a href="/index.html" target="_blank"
                                class="btn btn-danger  d-none d-md-block pull-right m-l-20 hidden-xs hidden-sm waves-effect waves-light">View Your Site</a>
                        </div>
                    </div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
               <div class="login-wrapper">
                    <div class="container">
                        <div class="row">
                            <!-- Row -->
                            <div class="row">
                                <!-- Column -->
                                <div class="col-lg-12 col-xlg-12 col-md-12">
                                    <div class="card">
                                        <div class="card-body">
                                            <form class="form-horizontal form-material" action="/admin/create-admin" method="POST">
                                            {{!-- OfferName --}}
                                                <div class="form-group mb-4">
                                                    <label class="col-md-12 p-0">Email Address</label>
                                                    <div class="col-md-12 border-bottom p-0">
                                                        <input type="text" placeholder="info@gmail.com"
                                                            class="form-control p-0 border-0" name="firstName">
                                                    </div>
                                                </div>
                                                {{!-- Discount amount --}}
                                                <div class="form-group mb-4">
                                                    <label class="col-md-12 p-0">Subject</label>
                                                    <div class="col-md-12 border-bottom p-0">
                                                        <input type="text" placeholder="e.g:Premium Category"
                                                            class="form-control p-0 border-0" name="firstName">
                                                    </div>
                                                </div>
                                                {{!-- Offer Details --}}
                                                <div class="form-group mb-4">
                                                    <label for="example-email" class="col-md-12 p-0">Message Details</label>
                                                    <div class="col-md-12 border-bottom p-0">
                                                        <textarea name="" id=""  class="form-control p-0 border-0" placeholder="Details wil be here.."></textarea>
                                                    </div>
                                                </div>
                                                <div class="form-group mb-4">
                                                    <div class="col-sm-12 d-flex justify-content-end">
                                                        <button class="btn btn-primary" type="submit">Send Message</button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <!-- Column -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- footer -->
            <!-- ============================================================== -->
            {{>footer}}