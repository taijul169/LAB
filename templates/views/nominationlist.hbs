            <div class="container-fluid">


                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <!-- Row -->
                <div class="row pt-2 m-1 ">
                    <div class="col-sm-12">
                        {{#if message}}
                            <div class="alert alert-success" role="alert">
                                {{message.message}}
                            </div>
                        {{/if}}
                        <div  class="card card-warning card-outline shadow">
                           <div class="card-header d-flex justify-content-between">
                            <a href="/admin/nominationlist/{{id}}" class="btn btn-secondary"><i class="fa fa-list-ul mr-2" aria-hidden="true"></i>Nomination list</a>
                            <a href="/admin/nominationhistorylist/{{id}}" class="btn btn-purple"><i class="fa fa-history mr-2" aria-hidden="true"></i>Nomination History</a>
                           </div>
                            <div class="card-body">
                             <div class="row">
                             {{#each designationData as |designationData|}}
                               {{!-- single-nomination --}}
                               <div class="col-md-3 col-12 col-sm-6 ">
                                 <div class="card border text-center">
                                   <div class="card-header">
                                    <b style="font-size: 12px;text-transform:uppercase">{{this.name}} <span> </span></b>
                                    
                                     <span style="font-size: 10px; padding:6px;"> </span>
                                   </div>
                                 </div>
                                  {{!-- single-user --}}
                                   {{#each ../allNominationData as |allNominationData|}}
                                      {{designationlist ../this.name this.designation this.id this.committee_id this.name this.userid this.vote this.delete_status this.nomination_attachment}}
                                      
                                   {{/each}}
                               </div>  
                              {{/each}}
                               <div class="col-md-12 col-sm-12 col-12 mt-4">
                                 <div class="nomination-action d-flex justify-content-end w-100">
                                   <div class="btn-group">
                                     <a class="btn btn-secondary mr-2" href="/admin/customresult/{{id}}">Custom Result </a>
                                     <a class="btn btn-primary " href="/admin/publishresult/{{id}}">Publish Result</a>
                                   </div>
                                 </div>
                               </div>
                             </div>
                               
                            </div>
                        </div>
                    </div>
                </div>
                {{!-- updated-nominationlist --}}
                  <div class="row pt-2 m-1 d-none">
                    <div class="col-sm-12">
                        {{#if message}}
                            <div class="alert alert-success" role="alert">
                                {{message.message}}
                            </div>
                        {{/if}}
                        <div  class="card card-warning card-outline shadow">
                           <div class="card-header">
                             Nomination list 
                           </div>
                            <div class="card-body">
                             <div class="row">
                             {{#each allNominationData}}
                               {{!-- single-nomination --}}
                               <div class="col-md-4 col-4 col-sm-6 ">
                                 <div class="card border text-center">
                                   <div class="card-header d-flex justify-content-between">
                                       <b style="font-size: 10px;">{{this.name}}</b>
                                       <a onclick=" return confirm('Are you sure?')" class="btn btn-sm btn-danger" href="/admin/deletenomination/{{this.id}}/{{this.committee_id}} " ><i class="far fa-times-circle"></i></a>
                                   </div>
                                   <span style="font-size: 10px; padding:6px;"> {{this.designation}}</span>
                                   <div class="card-footer d-flex justify-content-between">
                                     <a class="btn btn-sm btn-outline-primary" href="/users/singleuserview/{{this.userid}}">Details</a>
                                     <a class="btn btn-sm btn-info">Vote: {{this.vote}}</a>
                                     
                                   </div>
                                 </div>
                               </div>  
                              {{/each}}
                               <div class="col-md-12 col-sm-12 col-12 mt-4">
                                 <div class="nomination-action d-flex justify-content-end w-100">
                                   <div class="btn-group">
                                     <a class="btn btn-secondary mr-2" href="/admin/customresult/{{id}}">Custom Result </a>
                                     <a class="btn btn-primary " href="/admin/publishresult/{{id}}">Publish Result</a>
                                   </div>
                                 </div>
                               </div>
                             </div>
                               
                            </div>
                        </div>
                    </div>
                </div>
                </div>
            
                </div>
                <!-- Row -->
                <!-- ============================================================== -->
                <!-- End PAge Content -->
