             <script>

                const name_el =  document.getElementById('name'); 
                const id_el =  document.getElementById('com_id'); 
                const attachment_el =  document.getElementById('attachment');
                const designation_el =  document.getElementById('designation');
                const vote_el =  document.getElementById('vote');
                const displayDataIntoModal = (id,name,designation,attachment,vote)=>{
                    console.log(id)
                    id_el.value = id;
                    name_el.value = name;
                    designation_el.value = designation;
                    vote_el.value = vote;
                 
                    attachment_el.innerHTML = `<img src="data:image/png;base64,${attachment}" alt="user-img" width="36">`
                    
                }

